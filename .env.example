#.env.example
#-----------------------
# OpenAI
OPENAI_API_KEY=your_openai_key
OPENAI_MODEL=gpt-4o
OPENAI_EMBED_MODEL=text-embedding-3-small


# Search (옵션)
# DUCKDUCKGO는 키 불필요. Google CSE 사용 시 아래 2개 필요
GOOGLE_CSE_ID=
GOOGLE_API_KEY=


# 파이프라인 옵션
USE_WEB_SEARCH=0 # 1이면 DuckDuckGo 검색 사용
RETRIEVE_TOP_K=4
CHROMA_DIR=./chroma
KB_DIR=./kb